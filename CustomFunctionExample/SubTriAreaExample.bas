Attribute VB_Name = "SubTriAreaExample"
Sub CalcTriArea()
'
' Computates the area of a triangle using Triangle Area Function
'
    [A1] = "A side"
    [B1] = "B side"
    [C1] = "Angle"
    
    [A2] = "3"
    [B2] = "4"
    [C2] = "90"
    
    [D1] = "Area"
    [D2] = TriangleArea([A2], [B2], [C2])

End Sub
